    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "dependencies": {
    "core-js": "^3.4.7",
    "mapbox-gl": "^1.10.1",
    "node-fetch": "^2.6.0",
    "regenerator-runtime": "^0.13.3",
    "sass": "^1.54.3",
    "swiper": "^5.3.8"
  },
  "bugs": {
