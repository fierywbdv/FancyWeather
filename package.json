{
  "name": "webpack-boilerplate",
  "version": "1.0.1",
  "description": "WebPack: HTML, CSS, SCSS, JS, Img, Fonts",
  "main": ".eslintrc.js",
  "browserslist": [
    "> 0.25%, not dead"
  ],
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage",
    "lint:fix": "eslint ./**/*.js --fix",
    "dev": "webpack-dev-server --history-api-fallback --inline --open",
    "build": "npm run clear -s && webpack --progress",
    "lint": "eslint ./**/*.js",
    "clear": "del-cli dist"
  },
  "author": "GoldenkovVitali",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Net-zen/webpack-boilerplate.git"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.20.2",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.0.0",
    "mini-css-extract-plugin": "^0.8.0",
